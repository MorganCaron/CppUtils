export module CppUtils.Container.VariantTree;

import std;
import CppUtils.Container.Tree;
import CppUtils.Type;

export namespace CppUtils::Container
{
	template<class... Types>
	requires std::default_initializable<Type::NthType<0, Types...>>
	using VariantTree = Tree<std::variant<Types...>>;

	template<class... Types>
	using VariantTreeNode = VariantTree<Types...>::Node;
}
